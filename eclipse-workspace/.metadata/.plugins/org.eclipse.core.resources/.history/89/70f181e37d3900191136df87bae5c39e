package studentV2;

public class Driver {

	public static void main(String[] args) {
		Student s1 = new Student(); /* Creating an object (class instance) */

		s1.initName(); /* Calling non-static method */
		System.out.println("Name: " + s1.name);

		/* Calling static method */
		Student.printSchoolName();

		/* Try accessing printSchoolName without using class name */
		/* Try accessing printSchoolName using class instance */
		/* Try accessing initName() using class name */
	}
}
